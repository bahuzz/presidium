///////////////// main /////////////////////
h1 {
	font-size: 39px;
	padding-bottom: 20px;
}
h2 {
	font-size: 30px;
	padding-bottom: 15px;
}
h3 {
	font-size: 18px;
	margin-bottom: 20px;
}
.tar {
	text-align: right;
}
.tal {
	text-align: left;
}
.tac {
	text-align: center;
}
a {
	color: #9e2bb2;
	&:hover {
		text-decoration: none;
	}
}
.fl {
	float: left;
}
.fr {
	float: right;
}
.cb {
	clear: both;
}
.shad {
	-webkit-box-shadow: 0px 0px 5px rgba(0,0,0,.25);
	        box-shadow: 0px 0px 5px rgba(0,0,0,.25);
}
///////////////// btn ////////////////////
.btn {
	background: #007DC3;
	color: #fff;
	text-transform: uppercase;
	padding: 10px 16px;
	-webkit-box-shadow: 1px 1px 3px rgba(0,0,0,.25);
	        box-shadow: 1px 1px 3px rgba(0,0,0,.25);
	border: none;
	-webkit-border-radius: 2px;
	        border-radius: 2px;
	cursor: pointer;
}
.btn-tn {
	text-transform: none;
}
.menu-btn {
	display: none;
}
.logo {
	@include s(logo);
	text-indent: -9000px;
	display: inline-block;
	margin-bottom: 40px;
} 
.login-block {
	max-width: 380px;
	margin: auto;
	position: relative;
	top: 195px;
	padding: 10px;
	input[type="checkbox"] + label::before {
		top: 4px;
	}
}
.input-text {
	width: 100%;
	border: none;
	border-bottom: 1px solid #e1e1e1;
	padding: 10px 0;
	font-size: 14px;
}
.form-link {
	color: #3E3E3E;
	text-decoration: none;
	&:hover {
		text-decoration: underline;
	}
}
.form-row {
	margin-bottom: 25px;
}
input[type="checkbox"] {
    display:none;            
}
input[type="checkbox"] + label::before {
	content: "";
	display: inline-block;
	width: 18px;
	height: 18px;
	border: 1px solid #535858;
	margin-right: 4px;
	position: relative;
}
input[type="checkbox"]:disabled + label::before {
   	content: "";
	display: inline-block;
	width: 18px;
	height: 18px;
	border: 1px solid #949595;
	background: #efefef;
	margin-right: 4px;
	position: relative;
	opacity: 0.7;
	cursor: default;
}
input[type="checkbox"]:checked + label::before {
	background: #4aafe8;
}
input[type="checkbox"]:checked:disabled + label::before {
    background: #4aafe8;
    opacity: 0.7;
    cursor: default;
}
label {
	cursor: pointer;
}
.check {
	position: relative;
	top: 5px;
}
.form-title {
	font-size: 14px;
}
.parsley-error {
	border-bottom: 1px solid #F44336;
	&::-webkit-input-placeholder {color:#F44336;}
	&::-moz-placeholder          {color:#F44336;}/* Firefox 19+ */
	&:-moz-placeholder           {color:#F44336;}/* Firefox 18- */
	&:-ms-input-placeholder      {color:#F44336;}
}
